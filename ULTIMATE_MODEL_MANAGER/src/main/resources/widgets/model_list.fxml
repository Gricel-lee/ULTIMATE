<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:id="modelListVBox" fx:controller="gui.controllers.Model_List" VBox.vgrow="ALWAYS" spacing="10.0">
	
	<HBox alignment="CENTER" spacing="10.0" >
		<Button fx:id="addModelButton" onAction="#handleAddModel" mnemonicParsing="false" text="+" />
		<Button fx:id="upButton" onAction="#handleUp" mnemonicParsing="false" text="↑" />
		<Button fx:id="downButton" onAction="#handleDown" mnemonicParsing="false" text="↓" />
	</HBox>
	
	<ListView fx:id="modelListView" VBox.vgrow="ALWAYS" />
	
</VBox>